<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <WebView
        android:id="@+id/web"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="41dp"></WebView>


    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_gravity="bottom"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/cancle"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_gravity="bottom"
            android:background="@drawable/back_sel"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="16dp"
             />

        <TextView
            android:id="@+id/baoming"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_gravity="bottom|right"
            android:background="@drawable/back_sel"
            android:textColor="@color/black"
            android:gravity="center"
            android:text="报名"
            android:textSize="16dp"
            />

    </LinearLayout>
    <View
        android:layout_width="1.5px"
        android:layout_height="40dp"
        android:id="@+id/line"
        android:layout_gravity="bottom|center_horizontal"
        android:background="#aaaaaa"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1.5px"
        android:layout_gravity="bottom"
        android:layout_marginBottom="40dp"
        android:background="#aaaaaa"/>


</com.zhy.android.percent.support.PercentFrameLayout>